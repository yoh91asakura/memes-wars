{
  "name": "card-game-app",
  "version": "0.1.0",
  "description": "A modern card collection and battle game",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "dev:fast": "cross-env NODE_ENV=development vite --host",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=* vite",
    "build": "cross-env NODE_ENV=production npm run typecheck && vite build",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true npm run build",
    "preview": "cross-env NODE_ENV=production vite preview",
    "test": "cross-env NODE_ENV=test vitest",
    "test:unit": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest --watch",
    "test:coverage": "cross-env NODE_ENV=test vitest --coverage",
    "test:ci": "cross-env NODE_ENV=test CI=true vitest run --coverage",
    "test:contract": "cross-env NODE_ENV=test vitest run tests/contract/",
    "test:integration": "cross-env NODE_ENV=test vitest run tests/integration/",
    "test:contract:watch": "cross-env NODE_ENV=test vitest --watch tests/contract/",
    "test:integration:watch": "cross-env NODE_ENV=test vitest --watch tests/integration/",
    "validate:spec-kit": "npm run test:contract && npm run test:integration && npm run typecheck",
    "test:constitutional": "npm run test:contract && npm run test:integration && npm run test:e2e",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "validate": "concurrently \"npm run typecheck\" \"npm run lint\" \"npm run format:check\"",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "test:e2e:fast": "cross-env NODE_ENV=test playwright test --project=chromium --workers=4",
    "test:e2e:all": "cross-env NODE_ENV=test playwright test --project=chromium --project=firefox --project=webkit",
    "test:e2e:chromium": "cross-env NODE_ENV=test playwright test --project=chromium",
    "test:e2e:firefox": "cross-env NODE_ENV=test playwright test --project=firefox",
    "test:e2e:webkit": "cross-env NODE_ENV=test playwright test --project=webkit",
    "test:e2e:mobile": "cross-env NODE_ENV=test playwright test --project=mobile-chrome --project=tablet",
    "test:e2e:contract": "cross-env NODE_ENV=test playwright test tests/contract/",
    "test:e2e:integration": "cross-env NODE_ENV=test playwright test tests/integration/",
    "test:e2e:quick": "cross-env NODE_ENV=test playwright test --project=chromium --grep-invert=@slow",
    "test:e2e:single": "cross-env NODE_ENV=test playwright test --project=chromium --workers=1",
    "test:e2e:ui": "cross-env NODE_ENV=test playwright test --ui",
    "test:e2e:debug": "cross-env NODE_ENV=test playwright test --debug",
    "test:e2e:headed": "cross-env NODE_ENV=test playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:trace": "playwright show-trace",
    "test:e2e:watch": "cross-env NODE_ENV=test playwright test --ui --headed",
    "clean": "rimraf dist node_modules/.cache .nyc_output coverage",
    "clean:all": "rimraf dist node_modules/.cache .nyc_output coverage node_modules package-lock.json",
    "clean:install": "npm run clean:all && npm install",
    "clean:cache": "rimraf node_modules/.cache .nyc_output",
    "copy:assets": "cpx2 \"src/assets/**/*\" \"dist/assets\"",
    "mkdir": "shx mkdir -p",
    "echo": "shx echo",
    "spec:create": "node scripts/run-script.cjs create-new-feature",
    "spec:plan": "node scripts/run-script.cjs setup-plan",
    "spec:check": "node scripts/run-script.cjs check-task-prerequisites",
    "spec:update": "node scripts/run-script.cjs update-agent-context claude",
    "postinstall": "shx echo \"Setup complete! Run npm run dev to start development\"",
    "predev": "shx echo \"Starting development server...\"",
    "prebuild": "npm run typecheck",
    "start": "npm run dev",
    "ci": "concurrently \"npm run validate\" \"npm run test:ci\" \"npm run build\""
  },
  "keywords": [
    "card-game",
    "game",
    "collection",
    "battle"
  ],
  "author": "Development Team",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@tanstack/react-query": "5.85.3",
    "@tanstack/react-query-devtools": "5.85.3",
    "axios": "1.11.0",
    "framer-motion": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "7.8.2",
    "socket.io-client": "4.8.1",
    "styled-components": "6.1.19",
    "uuid": "^9.0.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "5.1.34",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.3.0",
    "concurrently": "8.2.2",
    "cpx2": "7.0.2",
    "cross-env": "10.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "glob": "11.0.3",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "playwright": "1.54.2",
    "prettier": "^3.6.2",
    "rimraf": "6.0.1",
    "shx": "0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.1.0",
    "vitest": "^1.3.0"
  }
}
