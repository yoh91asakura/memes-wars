{
  "name": "card-game-app",
  "version": "0.1.0",
  "description": "A modern card collection and battle game",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "validate": "npm run typecheck",
    "setup": "node -e \"process.platform === 'win32' ? require('child_process').spawn('powershell', ['scripts/setup-windows.ps1'], {stdio: 'inherit'}) : require('child_process').spawn('bash', ['scripts/setup-mac.sh'], {stdio: 'inherit'})\"",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup-windows.ps1",
    "setup:mac": "bash scripts/setup-mac.sh",
    "tasks:init": "node scripts/tasks/init.js",
    "tasks:list": "node scripts/tasks/list.js",
    "tasks:new": "node scripts/tasks/new.js",
    "tasks:complete": "node scripts/tasks/complete.js",
    "tasks:report": "node scripts/tasks/report.js",
    "task": "ts-node scripts/tasks/task-cli.ts",
    "task:create": "ts-node scripts/tasks/task-cli.ts create",
    "task:list": "ts-node scripts/tasks/task-cli.ts list",
    "task:analyze": "ts-node scripts/tasks/task-cli.ts analyze",
    "task:monitor": "ts-node scripts/tasks/task-cli.ts monitor",
    "task:sync": "ts-node scripts/tasks/task-cli.ts sync",
    "task:ai": "ts-node scripts/tasks/task-cli.ts ai-tasks",
    "task:stats": "ts-node scripts/tasks/task-cli.ts stats",
    "sparc:spec": "npx claude-flow sparc run spec-pseudocode",
    "sparc:architect": "npx claude-flow sparc run architect",
    "sparc:tdd": "npx claude-flow sparc tdd",
    "test:e2e": "playwright test",
    "test:e2e:fast": "playwright test --project=chromium --workers=4",
    "test:e2e:quick": "playwright test --project=chromium --grep-invert=@slow",
    "test:e2e:single": "playwright test --project=chromium --workers=1",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:watch": "playwright test --ui --headed",
    "clean": "cross-env-shell \"rmdir /s /q dist node_modules/.cache || rm -rf dist node_modules/.cache\"",
    "clean:install": "npm run clean && npm install",
    "postinstall": "cross-env-shell \"echo 'Setup complete! Run npm run dev to start development' || echo 'Setup complete! Run npm run dev to start development'\"",
    "predev": "npm run typecheck",
    "prebuild": "npm run typecheck",
    "mcp:status": "echo 'Checking MCP servers...' && npx @playwright/mcp --help && npx @0xshariq/github-mcp-server --help && npx claude-flow --version",
    "mcp:playwright": "npx @playwright/mcp",
    "mcp:github": "npx @0xshariq/github-mcp-server",
    "mcp:swarm": "npx claude-flow",
    "mcp:hooks:pre-task": "npx claude-flow@alpha hooks pre-task",
    "mcp:hooks:post-task": "npx claude-flow@alpha hooks post-task",
    "mcp:hooks:session-restore": "npx claude-flow@alpha hooks session-restore",
    "taskmaster:init": "./scripts/taskmaster-init.sh",
    "taskmaster:check": "./scripts/taskmaster-check.sh",
    "taskmaster:list": "task-master-ai get_tasks --projectRoot /Users/felixgirardin/Documents/meme-war-reloaded/memes-wars",
    "taskmaster:stats": "task-master-ai get_task_stats --projectRoot /Users/felixgirardin/Documents/meme-war-reloaded/memes-wars",
    "taskmaster:add": "task-master-ai add_task --projectRoot /Users/felixgirardin/Documents/meme-war-reloaded/memes-wars",
    "taskmaster:todo": "task-master-ai get_tasks --filter todo --projectRoot /Users/felixgirardin/Documents/meme-war-reloaded/memes-wars",
    "taskmaster:active": "task-master-ai get_tasks --filter in-progress --projectRoot /Users/felixgirardin/Documents/meme-war-reloaded/memes-wars",
    "taskmaster:critical": "task-master-ai get_tasks --filter critical --projectRoot /Users/felixgirardin/Documents/meme-war-reloaded/memes-wars",
    "taskmaster:high": "task-master-ai get_tasks --filter high --projectRoot /Users/felixgirardin/Documents/meme-war-reloaded/memes-wars"
  },
  "keywords": [
    "card-game",
    "game",
    "collection",
    "battle"
  ],
  "author": "Development Team",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@tanstack/react-query": "5.85.3",
    "@tanstack/react-query-devtools": "5.85.3",
    "axios": "1.11.0",
    "framer-motion": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "4.8.1",
    "styled-components": "6.1.19",
    "uuid": "^9.0.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@0xshariq/github-mcp-server": "2.3.0",
    "@eslint/js": "^9.33.0",
    "@modelcontextprotocol/sdk": "1.17.3",
    "@playwright/mcp": "0.0.34",
    "@playwright/test": "^1.54.2",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "5.1.34",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.3.0",
    "chalk": "^5.3.0",
    "claude-flow": "2.0.0-alpha.86",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "cross-env": "10.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "inquirer": "^9.2.15",
    "jsdom": "^26.1.0",
    "ora": "^8.0.1",
    "playwright": "1.54.2",
    "prettier": "^3.6.2",
    "ruv-swarm": "1.0.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.1.0",
    "vitest": "^1.3.0"
  }
}
